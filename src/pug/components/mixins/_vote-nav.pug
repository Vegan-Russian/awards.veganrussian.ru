mixin vote-nav(isVotingPage)
  -
    var nav_list = [
      {title: "Лучшее веганское молоко", link: "vegan_milk"},
      {title: "Лучшее бариста молоко", link: "vegan_milk_barista"},
      {title: "Лучший веганский кисломолочный продукт", link: "vegan_yigurt"},
      {title: "Лучший веганский сыр", link: "vegan_cheese"},
      {title: "Лучшее веганское мороженое", link: "vegan_ice_cream"},
      {title: "Лучшие веганские сырки", link: "vegan_cheese_curd"},
      {title: "Лучший веганский майонез", link: "vegan_mayo"},
      {title: "Лучший крем-чиз", link: "vegan_creme_cheese"},
