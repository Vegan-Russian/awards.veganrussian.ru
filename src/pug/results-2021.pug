doctype html
html.no-js(lang='ru')
  +_head('Результаты 2021')
  body.transition-lock
    block page
      header.header.anim
        .wrapper.header__wrapper
          a(href="/" title="На главную").header__logo: img(data-src="./images/gradient-logo-vegan-russian.svg", alt="Логотип сайта").lozad
          nav
            ul.header__list
              li: a(href="/#about").header__link О премии
              li: a(href="#contacts").header__link Контакты
              li: a(href="/results-2021.html").header__link 2021
              li: a(href="/results-2022.html").header__link 2022

          button(type="button").header__burger
            span.burger
              span.burger__line.burger__line_top
              span.burger__line
              span.burger__line.burger__line_bottom

      section.voting-header.voting-header_results
        .wrapper
          .voting-header__dots
            .voting-header__big-dot
            .voting-header__small-dot
          .voting-header__small-dot
          h1.anim результаты <span>vegan russian</span> awards&nbsp;2021
        img(data-src="./images/blob-3.webp", alt="").voting-header__blob-1.lozad
        img(data-src="./images/blob-4.webp", alt="").voting-header__blob-2.lozad

